{
  "expo": {
    "name": "almoxin",
    "slug": "almoxin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logoAlmoxin.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSDocumentsFolderUsageDescription": "O aplicativo precisa acessar os documentos para salvar e compartilhar arquivos PDF.",
        "NSFileProviderUsageDescription": "O aplicativo precisa acessar arquivos para salvar e compartilhar PDFs."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.codebr.almoxin",
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0a81307e-6f00-4c94-90a7-2a27714fc78e"
      }
    }
  }
}
